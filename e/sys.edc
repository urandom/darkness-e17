/* vim: set sw=3: */
styles
{
   style {
      name: "sys_style";
      base: "font=DejaVu-Bold font_size=16 text_class=tb_large align=center color=#ffffffc8 style=soft_shadow shadow_color=#263e5780 wrap=word";

      tag:  "br" "\n";
      tag:  "hilight" "+ font=DejaVu-Bold text_class=tb_large_light style=glow color=#fff glow2_color=#38526DA0 glow_color=#263E57EA";
   }
}

group {
   name: "e/sys/logout";
